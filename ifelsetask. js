for ( let m=1;m<=10;m++)
{
    if(m%5==0){
        console.log(m+" = is divisible by 5 ")
        
    }
    else{
        console.log(m+" = is not divisible by 5 ")
    }
    if(m%2==0)
        {
            console.log(m+" = is an even number ")
        }
        else
        {
            console.log(m+" = is an odd number ")
        }
        console.log("\n") //next line to make output clear.
}
